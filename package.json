    "@types/socket.io": "^3.0.1",
    "autoprefixer": "^10.4.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.0",
    "crypto-js": "^4.2.0",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.460.0",
    "next": "^14.2.0",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.4.0",
    "react": "^18.3.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.52.0",
