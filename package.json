{
  "name": "obdoc",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy-check": "node scripts/deploy-check.js",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "jest --selectProjects e2e",
    "test:all": "jest --selectProjects unit integration e2e",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:role-based": "ts-node src/__tests__/testRunner.ts",
    "test:security": "ts-node src/__tests__/testRunner.ts security",
    "test:signup-flow": "ts-node src/__tests__/testRunner.ts integration",
    "test:e2e-flow": "ts-node src/__tests__/testRunner.ts e2e",
    "test:failed": "ts-node src/__tests__/testRunner.ts failed",
    "test:supabase-auth": "ts-node src/__tests__/supabase-auth-test.ts",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:core-features": "node scripts/run-integration-tests.js",
    "test:deployment-ready": "npm run test:core-features && npm run test:security && npm run test:e2e-flow"
  },
  "dependencies": {
